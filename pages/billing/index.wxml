<view class="container">
  <view class='number'>
    <text>当前单车编号：{{number}}</text>
  </view>
  <view class='time'>
    <view class='time-title'>{{timeTitle}}</view>
    <view class='time-content'>{{hours}}:{{minutes}}:{{seconds}}</view>
  </view>
  <view class='endride'>
    <button type='warn' disabled='{{clickBtn}}' bindtap='endRide'>结束骑行</button>
    <button type='primary' bindtap='goBackIndex'>回到地图</button>    
  </view>
</view>